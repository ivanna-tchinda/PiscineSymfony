{% extends 'base.html.twig'%}
{% block body %}
<a href="/ex03">back to form<a/>
<p>TABLE</p>
<table>
    <tr>
        <th>Id</th>
        <th>Username</th>
        <th>Name</th>
        <th>Enable</th>
        <th>Email</th>
        <th>Birthdate</th>
        <th>Address</th>
    </tr>
    {% for user in users %}
    <tr>
        <td>{{ user.getId() }}</td>
        <td>{{ user.getUsername() }}</td>
        <td>{{ user.getName() }}</td>
        <td>{{ user.isEnable() == 1 ? "Yes" : "No" }}</td>
        <td>{{ user.getEmail() }}</td>
        <td>{{ user.Birthdate().format('Y-m-d') }}</td>
        <td>{{ user.getAddress() }}</td>
    </tr>
    {% endfor %}
</table>
{% endblock %}
